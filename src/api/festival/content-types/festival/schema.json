{
  "kind": "collectionType",
  "collectionName": "festivals",
  "info": {
    "singularName": "festival",
    "pluralName": "festivals",
    "displayName": "festivals",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "content": {
      "type": "customField",
      "options": {
        "preset": "custom"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "contentBefore": {
      "type": "customField",
      "options": {
        "preset": "custom"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "contentAfter": {
      "type": "customField",
      "options": {
        "preset": "custom"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "galery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "from": {
      "type": "date",
      "required": true
    },
    "to": {
      "type": "date",
      "required": true
    },
    "lineup": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::seller.seller"
    },
    "winners": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::seller.seller",
      "mappedBy": "winnerFestivals"
    },
    "event": {
      "type": "component",
      "repeatable": false,
      "component": "social.social"
    },
    "places": {
      "type": "integer",
      "required": true
    },
    "replacement": {
      "type": "boolean",
      "default": false
    },
    "meta": {
      "type": "component",
      "repeatable": false,
      "component": "seo.meta"
    }
  }
}
