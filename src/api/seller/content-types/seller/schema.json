{
  "kind": "collectionType",
  "collectionName": "sellers",
  "info": {
    "singularName": "seller",
    "pluralName": "sellers",
    "displayName": "sellers",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "customField",
      "options": {
        "preset": "custom"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "galery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "winnerFestivals": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::festival.festival",
      "inversedBy": "winners"
    },
    "labels": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::label.label",
      "inversedBy": "sellers"
    },
    "socials": {
      "type": "component",
      "repeatable": true,
      "component": "social.social"
    },
    "meta": {
      "type": "component",
      "repeatable": false,
      "component": "seo.meta"
    }
  }
}
